<Interface xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Interface.xsd">
	<Windows>

		<DynamicImage name="AnywhereTrainerTopBookendTemplate" texture="EA_Abilities01_d5" handleinput="false">
			<Size>
				<AbsPoint x="37" y="77" />
			</Size>
		</DynamicImage>
		<DynamicImage name="AnywhereTrainerBottomBookendTemplate" texture="EA_Abilities01_d5" handleinput="false">
			<Size>
				<AbsPoint x="37" y="113" />
			</Size>
		</DynamicImage>

		<Window name="AnywhereTrainerTabTemplate" layer="secondary">
			<Size>
				<AbsPoint x="43" y="48" />
			</Size>
			<Windows>
				<DynamicImage name="$parentIcon" texture="EA_Abilities01_d5" slice="Tab-ALL" layer="background" handleinput="false">
					<Anchors>
						<Anchor point="topleft" relativePoint="topleft" relativeTo="$parent" />
					</Anchors>
					<Size>
						<AbsPoint x="43" y="48" />
					</Size>
				</DynamicImage>
				<Button name="$parentInactiveImage" backgroundtexture="EA_Abilities01_d5" highlighttexture="EA_Abilities01_d5">
					<Anchors>
						<Anchor point="topleft" relativePoint="topleft" relativeTo="$parent" />
					</Anchors>
					<Size>
						<AbsPoint x="43" y="48" />
					</Size>
					<TexSlices>
						<Normal          id="RightTabFrame" />
						<NormalHighlit   id="RightTabFrame-Rollover" />
						<Pressed         id="RightTabFrame-Rollover" />
						<PressedHighlit  id="RightTabFrame-Rollover" />
						<Disabled        id="RightTabFrame" />
						<DisabledPressed id="RightTabFrame" />
					</TexSlices>
					<EventHandlers>
						<EventHandler event="OnMouseOver" function="AnywhereTrainer.OnMouseOver" />
					</EventHandlers>
				</Button>
			</Windows>
			<EventHandlers>
				<EventHandler event="OnLButtonUp" function="AnywhereTrainer.OnLButtonUp" />
			</EventHandlers>
		</Window>

	</Windows>
</Interface>